<!DOCTYPE suite SYSTEM "http://beust.com/testng/testng-1.0.dtd" >
<!-- The set of sanity tests to execute on the system -->
<suite name="Sanity Suite" parallel="methods" thread-count="4">
    <test name="Sanity Tests">

        <groups>
            <run>
                <include name="sanity"/>

            </run>
        </groups>
        <packages>
            <package name="test.*"/>
        </packages>
    </test>
</suite>
